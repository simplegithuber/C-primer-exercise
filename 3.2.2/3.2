//  一次读入一行
#include <iostream>
#include <string>

using std::string;
using std::cin;
using std::cout; using std::endl;

int main() {
	string line;
	while (getline(cin, line)) {
		cout << line << endl;
	}
	return 0;
}

---------------- 分割线
//  读入一个单词
#include <iostream>
#include <string>

using std::string;
using std::cin;
using std::cout; using std::endl;

int main() {
	string line;
	while (getline(cin, line)) {
        if(line.empty())
        break;
		cout << line << endl;
	}
	return 0;
}
