//  输出相邻整数的和
#include <iostream>
#include <vector>

using std::vector;
using std::cin;  using std::cout;
using std::endl;

int main() {
    vector<int> v;
    int i;
    char ch;
    cout << "请输入数字(输入‘#’退出)" << endl;
    while(cin >> i) {
        v.push_back(i);
    }
    for (auto it = v.begin(); it != v.end(); it++) {
        auto j = ++it;
        cout << j + it << endl;
    }
    return 0;
}
