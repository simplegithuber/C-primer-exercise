#include <iostream>
#include <string>
#inlcude <vector>
#include <cctype>

using std::string;  using std::vector;
using std::cin;  using std::cout;
using std::endl;

int main() {
    string str;
    vector<string> v;
    cout << "请输入一组单词" << endl;
    while(cin >> str) {
        if(str.empty()) {
            str = '\n';
            v.push_back(str);
        }
        else if(toupper(str)) 
            v.push_back(str);
    }
    for (auto c : v) {
        cout << c << endl;
    }
    return 0
}
